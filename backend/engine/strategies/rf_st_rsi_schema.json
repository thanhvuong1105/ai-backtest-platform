{
  "strategyName": "RF + ST + RSI Div Combined Strategy",
  "strategyType": "rf_st_rsi",
  "description": "Range Filter + SuperTrend + RSI Divergence combined strategy. Dual Flip entry when RF and ST flip within 8 bars, plus RSI divergence entries.",
  "author": "InschoShayne17122002",
  "version": "1.0",
  "inputs": {
    "entry": {
      "label": "Entry Settings",
      "params": [
        {
          "key": "showDualFlip",
          "label": "Enable Dual Flip Long Entry",
          "type": "bool",
          "default": true,
          "description": "Hiện Entry Dual Flip (Long)?"
        },
        {
          "key": "showRSI",
          "label": "Enable RSI Divergence Entry",
          "type": "bool",
          "default": true,
          "description": "Hiện Entry RSI Long?"
        },
        {
          "key": "st_atrPeriod",
          "label": "ST ATR Period",
          "type": "int",
          "default": 10,
          "min": 1,
          "max": 100,
          "step": 1
        },
        {
          "key": "st_src",
          "label": "ST Source",
          "type": "select",
          "default": "hl2",
          "options": ["close", "hl2", "hlc3", "ohlc4"]
        },
        {
          "key": "st_mult",
          "label": "ST ATR Multiplier",
          "type": "float",
          "default": 2.0,
          "min": 0.1,
          "max": 10.0,
          "step": 0.1
        },
        {
          "key": "st_useATR",
          "label": "ST use ATR()?",
          "type": "bool",
          "default": true
        },
        {
          "key": "rf_src",
          "label": "RF Source",
          "type": "select",
          "default": "close",
          "options": ["close", "hl2", "hlc3", "ohlc4"]
        },
        {
          "key": "rf_period",
          "label": "RF Period",
          "type": "int",
          "default": 100,
          "min": 1,
          "max": 500,
          "step": 1
        },
        {
          "key": "rf_mult",
          "label": "RF Multiplier",
          "type": "float",
          "default": 3.0,
          "min": 0.1,
          "max": 20.0,
          "step": 0.1
        },
        {
          "key": "rsi_length",
          "label": "RSI Length",
          "type": "int",
          "default": 14,
          "min": 1,
          "max": 100,
          "step": 1
        },
        {
          "key": "rsi_ma_length",
          "label": "MA Length on RSI",
          "type": "int",
          "default": 6,
          "min": 1,
          "max": 50,
          "step": 1
        }
      ]
    },
    "sl": {
      "label": "Stop Loss Settings",
      "params": [
        {
          "key": "sl_st_atrPeriod",
          "label": "ST SL ATR Period",
          "type": "int",
          "default": 10,
          "min": 1,
          "max": 100,
          "step": 1
        },
        {
          "key": "sl_st_src",
          "label": "ST SL Source",
          "type": "select",
          "default": "hl2",
          "options": ["close", "hl2", "hlc3", "ohlc4"]
        },
        {
          "key": "sl_st_mult",
          "label": "ST SL Multiplier",
          "type": "float",
          "default": 4.0,
          "min": 0.1,
          "max": 20.0,
          "step": 0.1
        },
        {
          "key": "sl_st_useATR",
          "label": "ST SL use ATR?",
          "type": "bool",
          "default": true
        },
        {
          "key": "sl_rf_period",
          "label": "RF SL Period",
          "type": "int",
          "default": 100,
          "min": 1,
          "max": 500,
          "step": 1
        },
        {
          "key": "sl_rf_mult",
          "label": "RF SL Multiplier",
          "type": "float",
          "default": 7.0,
          "min": 0.1,
          "max": 20.0,
          "step": 0.1
        }
      ]
    },
    "tp_dual": {
      "label": "Take Profit - Dual Flip",
      "params": [
        {
          "key": "tp_dual_st_atrPeriod",
          "label": "ST TP ATR Period (Dual Flip)",
          "type": "int",
          "default": 10,
          "min": 1,
          "max": 100,
          "step": 1
        },
        {
          "key": "tp_dual_st_mult",
          "label": "ST TP Multiplier (Dual Flip)",
          "type": "float",
          "default": 2.0,
          "min": 0.1,
          "max": 10.0,
          "step": 0.1
        },
        {
          "key": "tp_dual_rr_mult",
          "label": "TP R:R Multiplier (Dual Flip)",
          "type": "float",
          "default": 1.3,
          "min": 0.1,
          "max": 10.0,
          "step": 0.1,
          "description": "Risk-Reward ratio for Take Profit"
        }
      ]
    },
    "tp_rsi": {
      "label": "Take Profit - RSI",
      "params": [
        {
          "key": "tp_rsi_st_atrPeriod",
          "label": "ST TP ATR Period (RSI)",
          "type": "int",
          "default": 10,
          "min": 1,
          "max": 100,
          "step": 1
        },
        {
          "key": "tp_rsi_st_mult",
          "label": "ST TP Multiplier (RSI)",
          "type": "float",
          "default": 2.0,
          "min": 0.1,
          "max": 10.0,
          "step": 0.1
        },
        {
          "key": "tp_rsi_rr_mult",
          "label": "TP R:R Multiplier (RSI)",
          "type": "float",
          "default": 1.3,
          "min": 0.1,
          "max": 10.0,
          "step": 0.1,
          "description": "Risk-Reward ratio for RSI Take Profit"
        }
      ]
    },
    "debug": {
      "label": "Debug Settings",
      "params": [
        {
          "key": "debug",
          "label": "Enable Debug Logging",
          "type": "bool",
          "default": false,
          "description": "Print detailed trade logs for comparison with TradingView"
        }
      ]
    }
  },
  "defaultParams": {
    "showDualFlip": true,
    "showRSI": true,
    "st_atrPeriod": 10,
    "st_src": "hl2",
    "st_mult": 2.0,
    "st_useATR": true,
    "rf_src": "close",
    "rf_period": 100,
    "rf_mult": 3.0,
    "rsi_length": 14,
    "rsi_ma_length": 6,
    "sl_st_atrPeriod": 10,
    "sl_st_src": "hl2",
    "sl_st_mult": 4.0,
    "sl_st_useATR": true,
    "sl_rf_period": 100,
    "sl_rf_mult": 7.0,
    "tp_dual_st_atrPeriod": 10,
    "tp_dual_st_mult": 2.0,
    "tp_dual_rr_mult": 1.3,
    "tp_rsi_st_atrPeriod": 10,
    "tp_rsi_st_mult": 2.0,
    "tp_rsi_rr_mult": 1.3,
    "debug": false
  },
  "mapping": {
    "pineScript": {
      "showEntryLong": "showDualFlip",
      "showEntryRSI": "showRSI",
      "st_atrPeriod": "st_atrPeriod",
      "st_src": "st_src",
      "st_mult": "st_mult",
      "st_changeATR": "st_useATR",
      "rf_src_in": "rf_src",
      "rf_period": "rf_period",
      "rf_mult": "rf_mult",
      "lenRSI": "rsi_length",
      "lenMA": "rsi_ma_length",
      "st_sl_atrPeriod": "sl_st_atrPeriod",
      "st_sl_src": "sl_st_src",
      "st_sl_mult": "sl_st_mult",
      "st_sl_useATR": "sl_st_useATR",
      "rf_sl_period": "sl_rf_period",
      "rf_sl_mult": "sl_rf_mult",
      "st_tp_dual_period": "tp_dual_st_atrPeriod",
      "st_tp_dual_mult": "tp_dual_st_mult",
      "rr_mult_dual": "tp_dual_rr_mult",
      "st_tp_rsi1_period": "tp_rsi_st_atrPeriod",
      "st_tp_rsi1_mult": "tp_rsi_st_mult",
      "rr_mult_rsi1": "tp_rsi_rr_mult"
    }
  }
}
