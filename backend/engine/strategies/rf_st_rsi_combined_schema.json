{
  "strategyName": "RF + ST + RSI Div Combined Strategy (Long & Short)",
  "strategyType": "rf_st_rsi_combined",
  "description": "Range Filter + SuperTrend + RSI Divergence combined strategy with Long & Short positions. Dual Flip entry when RF and ST flip within 8 bars (Long) or 12 bars (Short), plus RSI divergence entries.",
  "author": "InschoShayne17122002",
  "version": "2.0",
  "inputs": {
    "enable": {
      "label": "Enable Settings",
      "params": [
        {
          "key": "enableLong",
          "label": "Enable Long Trading",
          "type": "bool",
          "default": true,
          "description": "Allow Long positions"
        },
        {
          "key": "enableShort",
          "label": "Enable Short Trading",
          "type": "bool",
          "default": true,
          "description": "Allow Short positions"
        }
      ]
    },
    "entry": {
      "label": "Entry Settings",
      "params": [
        {
          "key": "showEntryLong",
          "label": "Enable Dual Flip Long Entry",
          "type": "bool",
          "default": true,
          "description": "Hiện Entry Dual Flip (Long)?"
        },
        {
          "key": "showEntryShort",
          "label": "Enable Dual Flip Short Entry",
          "type": "bool",
          "default": true,
          "description": "Hiện Entry Dual Flip (Short)?"
        },
        {
          "key": "showEntryRSI_L",
          "label": "Enable RSI Long Entry",
          "type": "bool",
          "default": true,
          "description": "Hiện Entry RSI Long?"
        },
        {
          "key": "showEntryRSI_S",
          "label": "Enable RSI Short Entry",
          "type": "bool",
          "default": true,
          "description": "Hiện Entry RSI Short?"
        },
        {
          "key": "dualFlipBarsLong",
          "label": "Dual Flip Bars (Long)",
          "type": "int",
          "default": 8,
          "min": 1,
          "max": 50,
          "step": 1,
          "description": "Max bars between RF and ST flip for Long entry"
        },
        {
          "key": "dualFlipBarsShort",
          "label": "Dual Flip Bars (Short)",
          "type": "int",
          "default": 12,
          "min": 1,
          "max": 50,
          "step": 1,
          "description": "Max bars between RF and ST flip for Short entry"
        },
        {
          "key": "st_atrPeriod",
          "label": "ST ATR Period",
          "type": "int",
          "default": 10,
          "min": 1,
          "max": 100,
          "step": 1
        },
        {
          "key": "st_src",
          "label": "ST Source",
          "type": "select",
          "default": "hl2",
          "options": ["close", "hl2", "hlc3", "ohlc4"]
        },
        {
          "key": "st_mult",
          "label": "ST ATR Multiplier",
          "type": "float",
          "default": 2.0,
          "min": 0.1,
          "max": 10.0,
          "step": 0.1
        },
        {
          "key": "st_changeATR",
          "label": "ST use ATR()?",
          "type": "bool",
          "default": true
        },
        {
          "key": "rf_src_in",
          "label": "RF Source",
          "type": "select",
          "default": "close",
          "options": ["close", "hl2", "hlc3", "ohlc4"]
        },
        {
          "key": "rf_period",
          "label": "RF Period",
          "type": "int",
          "default": 100,
          "min": 1,
          "max": 500,
          "step": 1
        },
        {
          "key": "rf_mult",
          "label": "RF Multiplier",
          "type": "float",
          "default": 3.0,
          "min": 0.1,
          "max": 20.0,
          "step": 0.1
        },
        {
          "key": "lenRSI",
          "label": "RSI Length",
          "type": "int",
          "default": 14,
          "min": 1,
          "max": 100,
          "step": 1
        },
        {
          "key": "lenMA",
          "label": "MA Length on RSI",
          "type": "int",
          "default": 6,
          "min": 1,
          "max": 50,
          "step": 1
        }
      ]
